    <ul class="similar_product_list">
    {set _index=1}
    {for ixProduct:product in similarSet.productItems}
        {if product.requireAdultAuth}
        <li class="similar_product_item">
            <span class="similar_product_index">{=_index}</span>
            {set _index = _index + 1}
            <a href="/daon82/products/{=product.id}" class="similar_product N=a:sim.item">
                <div class="thumb shield_thumb">
                </div>
                <div class="similar_info">
                    성인인증 후 확인 가능합니다.
                </div>
            </a>
        </li>
        {elseif product.requireYouthSafe}
        <li class="similar_product_item">
            <span class="similar_product_index">{=_index}</span>
            {set _index = _index + 1}
            <a href="/daon82/products/{=product.id}" class="similar_product N=a:sim.item">
                <div class="thumb adult_thumb">
                </div>
                <div class="similar_info">
                    성인인증 후 확인 가능합니다.
                </div>
            </a>
        </li>
        {else}
        <li class="similar_product_item">
            <span class="similar_product_index">{=_index}</span>
            {set _index = _index + 1}
            <a href="/daon82/products/{=product.id}" class="similar_product N=a:sim.item">
                <div class="thumb {if product.productStatusType == 'OUTOFSTOCK'}soldout{/if}">
                    <!-- 가로가 긴 경우 -->
                    <img src="{=product.representImageUrl}?type=f212_212" alt="" class="similar_image">
                </div>
                <div class="similar_info">
                    {if product.colorOptionStandardList.length >= 1}
                        {if layoutVersion === "VERSION2"}
                            {set _baseCount = 24}
                        {else}
                            {set _baseCount = 18}
                        {/if}

                        {if product.colorOptionStandardList.length > _baseCount + 1}
                            {set _moreOptionCount = product.colorOptionStandardList.length - _baseCount}
                        {else}
                            {set _moreOptionCount = 0}
                        {/if}
                        {set _optionIndex=1}
                    <div class="similar_colors">
                        <span class="blind">색상</span>
                    {for ixColor:colorOption in product.colorOptionStandardList}
                        <!-- [D] 한줄에 최대 25개 노출, 26개 이상은 24개 +N개 -->
                        {if colorOption.imageInfo.colorType == "STYLE"}
                        <span class="similar_product_color" style="background-color: {=colorOption.imageInfo.colorValue};">
                            <span class="blind">{=colorOption.optionName}</span>
                        </span>
                        {else}
                            {if colorOption.imageInfo.colorValue == "color-type-trans"}
                                {set _colorClassValue = "transparent"}
                            {elseif colorOption.imageInfo.colorValue == "color-type-multi"}
                                {set _colorClassValue = "multi"}
                            {else}
                                {set _colorClassValue = "white"}
                            {/if}
                        <span class="similar_product_color {=_colorClassValue}">
                            <span class="blind">{=colorOption.optionName}</span>
                        </span>
                        {/if}
                        {if _optionIndex == _baseCount && _moreOptionCount >= 1}
                            <!-- [D] 컬러칩이 16개 이상일 경우 24개 다음으로 아래 태그 위치 -->
                            <span class="more_color">+{=_moreOptionCount}<span class="blind">개 색상이 더 있음</span></span>
                        {/if}

                        {set _optionIndex = _optionIndex + 1}
                    {/for}
                    </div>
                    {/if}
                    <strong class="similar_product_name">{=product.name}</strong>
                    <strong class="similar_product_price">
                        <span class="similar_number">{js nmp.utility.convertNumberFormat(=product.discountedPrice)}</span>원
                    </strong>
                    {if product.discountPercentage >= 1}
                    <span class="similar_product_percent">{=product.discountPercentage}%<span class="blind">할인</span></span>
                    {/if}
                    {if product.reviewCount >= 1}
                    <div class="similar_review_number">리뷰 {=product.reviewCount}</div>
                    {/if}
                </div>
            </a>
            {if product.zzim}
                {set _pressed=true}
            {else}
                {set _pressed=false}
            {/if}
            <a class="_click(nmp.front.sellershop.toggleKeep({=product.id})) _stopDefault _responsive_scrap_button scrap N=a:sim.fav" role="button" data-scrap-item-id="{=product.id}" aria-pressed="{=_pressed}" href="#찜하기">
                <span class="blind">찜하기</span>
            </a>
            {if product.enableCart && !product.requireSubscribe && product.productStatusType == 'SALE'}
            <a role="button" href="#" class="_multi_cart_button _click(nmp.front.sellershop.product.show.similar.bridgecart.similarCartAdd({=product.id})) _stopDefault basket N=a:sim.cart">
                <span class="blind">장바구니에 담기</span>
            </a>
            {/if}
        </li>
        {/if}
    {/for}
    </ul>
</div>
</script>


<script type="text/template" class="_tpl_layer_similar_standard_component">
    <div class="layer_inner">
        <div class="scroll">
            <strong class="layer_title _layer_title">색상을 선택하세요</strong>
            <div role="radiogroup" class="{=colorWrapClass} _colorButtons">
            {for ixColor:colorOption in colorOptions}
                {if colorWrapClass === "image_wrap"}
                <!-- [D] 선택 불가 요소일 경우 disabled 속성 추가 부탁드립니다. 선택된 요소의 경우 aria-checked="true"/선택되지 않은요소 : aria-checked="false" 값 으로 설정해주세요. -->
                <button type="button" aria-checked="false" color-value="{=colorOption.optionName}" class="_click(nmp.front.sellershop.product.show.similar.bridgecart.selectColor()) _stopDefault image_button N=a:sim.colorimg" role="radio" {if colorOption.disable}disabled=""{/if}>
                    <!-- [D] 세로가 더 짧은경우 img_full_h 클래스 적용 부탁드립니다. -->
                    <!-- [D] alt에 색상명 적용 부탁드립니다(접근성) -->
                    <img src="{=colorOption.imageInfo.imageUrls[0]}?type=f120_120" alt="{=colorOption.optionName}" class="product_image">
                </button>
                {else}
                <!-- [D] 선택 불가 요소일 경우 disabled 속성 추가 부탁드립니다. 선택된 요소의 경우 aria-checked="true"/선택되지 않은요소 : aria-checked="false" 값 으로 설정해주세요. -->
                <button type="button" aria-checked="false" color-value="{=colorOption.optionName}" role="radio" class=" _click(nmp.front.sellershop.product.show.similar.bridgecart.selectColor()) _stopDefault color_button N=a:sim.color"  {if colorOption.disable}disabled=""{/if}>
                    <!-- [D] span.product_color에 인라인 스타일(background-color)로 색상값 적용 부탁드립니다. -->
                    {if colorOption.imageInfo.colorType == "STYLE"}
                    <span class="product_color" style="background-color: {=colorOption.imageInfo.colorValue};">
                        <!-- span.blind에 색상이름 적용 부탁드립니다.(접근성) -->
                        <span class="blind">{=colorOption.optionName}</span>
                    </span>
                    {else}
                        {if colorOption.imageInfo.colorValue == "color-type-trans"}
                            {set _colorClassValue = "transparent"}
                        {elseif colorOption.imageInfo.colorValue == "color-type-multi"}
                            {set _colorClassValue = "multi"}
                        {else}
                            {set _colorClassValue = "white"}
                        {/if}
                    <span class="product_color {=_colorClassValue}">
                        <!-- span.blind에 색상이름 적용 부탁드립니다.(접근성) -->
                        <span class="blind">{=colorOption.optionName}</span>
                    </span>
                    {/if}
                </button>
                {/if}
            {/for}
            </div>
            <strong class="layer_title _size_layer_title">사이즈를 선택하세요</strong>
            <div role="radiogroup" class="size_wrap _sizeButtons">
            {for ixColor:sizeOption in sizeOptions}
